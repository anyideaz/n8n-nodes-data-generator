{
	"name": "n8n-nodes-data-generator",
	"version": "1.0.0",
	"description": "Generate mock data using Faker.js",
	"keywords": [
		"n8n-community-node-package",
		"data-generator",
		"faker.js",
		"mock",
		"n8n"
	],
	"license": "MIT",
	"homepage": "https://sqldatagenerator.com",
	"author": {
		"name": "coderzz",
		"email": "blueberry267@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/anyideaz/n8n-nodes-data-generator.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes --write",
		"lint": "eslint nodes package.json",
		"lintfix": "eslint nodes package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"categories": [
			"Development"
		],
		"nodes": [
			"dist/nodes/DataGenerator/DataGenerator.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "~5.45",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^2.7.1",
		"typescript": "~4.8.4"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"@coderzz/mocker-data-generator": "^3.0.5",
		"@faker-js/faker": "^8.4.1"
	}
}
